                    "custom_liquid": "{% assign preorder = product.selling_plan_groups | where: 'name', 'Preorder' %}\n\n{% if preorder.size > 0 %}\n{% style %}\n    div:has( > .preorder-group) {\n        width: 100%;\n    }\n    .text-pre-price {\n        font-size: var(--font-size--sm);\n    }\n    .price-pre {\n        font-weight: var(--font-accent--weight);\n        font-family: var(--font-accent--family);\n        font-size: var(--font-size--sm);\n    }\n    .discounted-pre {\n        color: var(--color-foreground-heading);\n        font-size: var(--font-size--lg);\n    }\n    .preorder-group {\n        display: flex;\n        flex-direction: column;\n        gap: var(--gap-lg);\n        background: #fafafa;\n        border-radius: 8px;\n        padding: 20px;\n    }\n    .flex-price-pre {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        line-height: 1em;\n    }\n    .preorder-label {\n        display: block;\n        text-transform: uppercase;\n        font-size: var(--font-size--xs);\n        font-weight: bold;\n        opacity: .4;\n    }\n    .caption-final-pre {\n        font-size: var(--font-size--xs);\n        opacity: .4;\n        font-style: italic;\n        display: block;\n        margin-top: var(--gap-xs);\n    }\n{% endstyle %}\n  {% assign selling_plan = preorder[0].selling_plans[0] %}\n  {% if selling_plan.checkout_charge.value_type == 'percentage' %}\n    {% assign original_price = product.price %}\n    {% assign pay_now_percent = selling_plan.checkout_charge.value %}\n    {% assign final_percent = 100 | minus: pay_now_percent %}\n\n    {% assign pay_now_amount = original_price | times: pay_now_percent | divided_by: 100 %}\n    {% assign final_amount = original_price | times: final_percent | divided_by: 100 %}\n\n    <div class=\"preorder-group\">\n        <div>\n            <span class=\"preorder-label\">Pre-Order Payment Plan</span>\n        </div>\n        <div class=\"flex-price-pre\">\n            <span class=\"text-pre-price\">\n                Pay Today\n            </span>\n            <span class=\"discounted-pre price-pre\">\n                {{ pay_now_amount | money }}\n            </span>\n        </div>\n        <div>\n            <div class=\"flex-price-pre\">\n                <span class=\"text-pre-price\">\n                    Final payment\n                </span>\n                <span class=\"price-pre\">\n                    {{ final_amount | money }}\n                </span>\n            </div>\n            <span class=\"caption-final-pre\">\n                Charged when shipped Q1 2026\n            </span>\n        </div>\n    </div>\n  {% endif %}\n{% endif %}"
